{
  "name": "wp-vue-form-builder",
  "version": "0.0.0fro",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --config vite.config.js",
    "dev:front": "vite --config vite.config.frontend.js",
    "build": "run-p type-check \"build-only {@}\"  --",
    "build:front": "run-p type-check \"build-only:front {@}\" --",
    "preview": "vite preview --config vite.config.js",
    "preview:front": "vite preview --config vite.config.frontend.js",
    "test:unit": "vitest",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "build-only": "vite build --config vite.config.js",
    "build-only:front": "vite build --config vite.config.frontend.js",
    "build-all": "npm run build-only && npm run build-only:front",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@vueform/vueform": "^1.9.2",
    "axios": "^1.6.8",
    "pinia": "^2.1.7",
    "primeicons": "^7.0.0",
    "primevue": "^3.46.0",
    "vue": "^3.4.21",
    "vue-loading-overlay": "6.0",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node20": "^20.1.2",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.11.28",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.5",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.19",
    "cypress": "^13.7.0",
    "eslint": "^8.49.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^24.0.0",
    "npm-run-all2": "^6.1.2",
    "postcss": "^8.4.38",
    "prettier": "^3.0.3",
    "start-server-and-test": "^2.0.3",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.8.3",
    "vite": "^7.0.4",
    "vite-plugin-vue-devtools": "^7.0.18",
    "vitest": "^1.4.0",
    "vue-tsc": "^2.2.12"
  }
}
